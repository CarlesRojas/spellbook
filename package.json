{
    "name": "spellbook",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "npm run lint && npm run ts && next build",
        "start": "next start",
        "lint": "next lint && prettier --check '**/*.{ts,tsx}'",
        "ts": "tsc --noEmit --incremental",
        "pretty": "prettier --write '**/*.{ts,tsx}'",
        "db:generate": "drizzle-kit generate",
        "db:migrate": "ts-node -r dotenv/config -P ./tsconfig.json ./server/database/migration/applyMigrations.ts",
        "db:studio": "drizzle-kit studio",
        "spells": "ts-node -r dotenv/config -P ./tsconfig.json ./server/scraper"
    },
    "dependencies": {
        "@ducanh2912/next-pwa": "^10.2.7",
        "@radix-ui/react-avatar": "^1.0.4",
        "@radix-ui/react-dialog": "^1.0.5",
        "@radix-ui/react-dropdown-menu": "^2.0.6",
        "@radix-ui/react-slot": "^1.0.2",
        "@t3-oss/env-nextjs": "^0.10.1",
        "@tailwindcss/typography": "^0.5.13",
        "@tanstack/react-query": "^5.35.5",
        "accept-language": "^3.0.18",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "dotenv": "^16.4.5",
        "drizzle-orm": "^0.30.10",
        "google-auth-library": "^9.9.0",
        "jiti": "^1.21.0",
        "just-debounce-it": "^3.2.0",
        "next": "14.2.3",
        "next-auth": "^4.24.7",
        "next-themes": "^0.3.0",
        "postgres": "^3.4.4",
        "react": "^18",
        "react-dom": "^18",
        "react-hook-form": "^7.51.4",
        "react-icons": "^5.2.1",
        "tailwind-merge": "^2.3.0",
        "tailwindcss-animate": "^1.0.7",
        "zod": "^3.23.8"
    },
    "devDependencies": {
        "@savvywombat/tailwindcss-grid-areas": "^4.0.0",
        "@svgr/webpack": "^8.1.0",
        "@types/node": "^20",
        "@types/react": "^18",
        "@types/react-dom": "^18",
        "autoprefixer": "^10.4.19",
        "drizzle-kit": "^0.21.1",
        "eslint": "^8",
        "eslint-config-next": "14.2.3",
        "pg": "^8.11.5",
        "postcss": "^8",
        "prettier": "^3.2.5",
        "prettier-plugin-tailwindcss": "^0.5.14",
        "tailwindcss": "^3.4.1",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5"
    }
}
